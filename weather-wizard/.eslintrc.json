{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes" : ["warn", "double"],
        "no-restricted-syntax" : ["off", "ForOfStatement"],
        "no-plusplus" : "off",
        "no-console" : "off",
        "no-undef" : "off"    
    }
}
